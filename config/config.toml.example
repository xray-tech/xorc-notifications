[postgres]
uri = "postgres://root:kulli@localhost:5432/crm"
cache_ttl = 10
pool_size = 10
min_idle = 2
idle_timeout = 90000
max_lifetime = 1800000

[rabbitmq]
queue = "ha.webpush.queue"
exchange = "ha.push-notifications"
exchange_type = "topic"
response_exchange = "ha.push-notification-responses.fanout"
response_exchange_type = "fanout"
routing_key = "web"
vhost = "/"
host = "localhost"
port = 5672
login = "guest"
password = "guest"

[log]
host = "graylog.service.consul:12201"